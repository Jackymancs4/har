{
    "name": "deviantintegral/har",
    "description": "A PHP library for working with HTTP Archives",
    "type": "library",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "deviantintegral@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "jms/serializer": "^3.0",
        "doctrine/annotations": "^1.7",
        "guzzlehttp/psr7": "^1.6",
        "deviantintegral/jms-serializer-uri-handler": "^1.0",
        "deviantintegral/null-date-time": "^0.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^8.3",
        "symfony/console": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Deviantintegral\\Har\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deviantintegral\\Har\\Tests\\": "tests/src"
        }
    }
}
